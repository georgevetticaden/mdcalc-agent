# MDCalc Agent Environment Variables

# Snowflake Configuration (already configured in health-mcp)
# These are for reference - actual values are in Claude Desktop config
SNOWFLAKE_USER=your_username
SNOWFLAKE_ACCOUNT=your_account
SNOWFLAKE_PRIVATE_KEY_PATH=~/.ssh/snowflake/snowflake_key.p8
SNOWFLAKE_WAREHOUSE=COMPUTE_WH
SNOWFLAKE_DATABASE=HEALTH_INTELLIGENCE
SNOWFLAKE_SCHEMA=HEALTH_RECORDS
SNOWFLAKE_ROLE=ACCOUNTADMIN

# MDCalc Configuration
MDCALC_BASE_URL=https://www.mdcalc.com
MDCALC_RATE_LIMIT=1  # requests per second
MDCALC_TIMEOUT=30000  # milliseconds

# Playwright Configuration
PLAYWRIGHT_HEADLESS=true
PLAYWRIGHT_SLOW_MO=0  # milliseconds between actions
PLAYWRIGHT_SCREENSHOT_ON_ERROR=true

# Development Settings
DEBUG=false
LOG_LEVEL=info